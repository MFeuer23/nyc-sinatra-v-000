
  <h1>Create a new Landmark</h1>
  <form action="/landmarks/<%=@landmark.id%>" method="post">
    <p>Landmark Name: <input type="text" name="landmark[name]" id="landmark_name" value="<%=@landmark.name%>"></p>
    <p>Year Completed: <input type="text" name="landmark[year_completed]" id="landmark_year_completed" value="<%=@landmark.year_completed%>"></p>


    <input type="submit" value="Edit Landmark" id="Edit Landmark">

    </form>
